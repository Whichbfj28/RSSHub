{   
    "framework": null,
    "github": { "silent": true },
    "routes": [{ "src": "/.*", "dest": "/api/vercel.ts" }]

  "functions": {
    "api/*.js": {
      "maxDuration": 60 // This function can run for a maximum of 15 seconds
    },
    "src/api/*.js": {
      "maxDuration": 60 // You must prefix functions in the src directory with /src/
    }
  }
}

